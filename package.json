{
  "name": "backend-app",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "dev": "nodemon src/index.ts --exec ts-node",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js",
    "migration:generate": "npm run typeorm -- migration:generate ./src/migrations/users -d ./src/config/database.ts",
    "migration:run": "npm run typeorm -- migration:run -d ./src/config/database.ts",
    "migration:revert": "npm run typeorm -- migration:revert -d ./src/config/database.ts",
    "seed": "ts-node src/seeders/index.ts",
    "seed:user": "ts-node src/seeders/index.ts user"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "description": "",
  "dependencies": {
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "mysql2": "^3.11.5",
    "reflect-metadata": "^0.2.2",
    "typeorm": "^0.3.20"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "nodemon": "^3.1.7",
    "ts-node": "^10.9.2"
  }
}
